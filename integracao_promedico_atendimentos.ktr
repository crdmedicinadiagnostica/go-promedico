  <step>
    <name>integracao_promedico</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>dicomvix</connection>
    <sql>SELECT distinct cd_paciente
              , upper(ds_paciente)                      as ds_paciente
              , dt_nascimento
              , 2                                 	as cd_medico
              , upper(ds_solicitante)                   as ds_medico
              , split_part(ds_crm_solicitante, &apos;-&apos;, 1)  as ds_crm_nr
              , split_part(ds_crm_solicitante, &apos;-&apos;, 2)  as ds_crm_uf
              , &apos;CRM&apos;                                   as ds_conselho
              , codigoexame                             as nr_controle
              , dt_exame::date                          as dt_exame
			  , created_at::time                        as dt_hora
              , replace(cd_procedimento, &apos;.&apos;, &apos;&apos;):: int as cd_procedimento
              , upper(ds_exame)                         as ds_procedimento
              , case
                  when ds_modalidade = &apos;CR&apos; then 3
                  when ds_modalidade = &apos;DX&apos; then 3
              end  as cd_modalidade
              , case
                  when ds_modalidade = &apos;CR&apos; then 1
                  when ds_modalidade = &apos;DX&apos; then 1
              end  as cd_sala
              --, case
              --  when ds_modalidade = &apos;CT&apos; then 4
              --when ds_modalidade = &apos;US&apos; then 5
              --when ds_modalidade = &apos;CR&apos; then 3
              --when ds_modalidade = &apos;MR&apos; then 2
              --when ds_modalidade = &apos;OT&apos; then 1
              --end                                                   as cd_procedimento
              , 1                                       as cd_plano


FROM integracao_promedico
where created_at &gt;= now()::date -1
  and ds_modalidade in (&apos;CR&apos;,&apos;DX&apos;)
  and ds_solicitante is not null
  and ds_paciente is not null
  and sn_integrado is false
  and codigoexame &lt;&gt; 0;</sql>
